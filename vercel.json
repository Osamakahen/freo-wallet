{
  "version": 3,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_MAINNET_RPC_URL": "https://eth-mainnet.g.alchemy.com/v2/demo",
    "NEXT_PUBLIC_WALLET_NAME": "Freo Wallet",
    "NEXT_PUBLIC_WALLET_DESCRIPTION": "A secure and user-friendly Ethereum wallet",
    "NEXT_PUBLIC_ENABLE_TRANSACTION_MONITORING": "true",
    "NEXT_PUBLIC_ENABLE_TOKEN_MANAGEMENT": "true",
    "NEXT_PUBLIC_ENCRYPTION_KEY": "freo-wallet-production-key-2024",
    "NEXT_PUBLIC_SESSION_TIMEOUT": "3600",
    "NEXT_PUBLIC_THEME": "light",
    "NEXT_PUBLIC_DEFAULT_CURRENCY": "USD"
  }
} 